  "name": "socialnetwork-react",
  "version": "1.0.3",
  "description": "MDL ReactJS",
  "scripts": {
    "dev": "cross-env REDIS_HOST=localhost DISABLE_CACHE=true DEBUG=true WEBPACK_PORT=3004 NODE_PATH=./app concurrent --kill-others \"nodemon app/server\" \"node webpack.server.js\" \"gulp css:watch\"",
    "dev-local":"REDIS_HOST=localhost DISABLE_CACHE=true DEBUG=true WEBPACK_PORT=3004 cross-env DEV=true NODE_PATH=./app concurrent --kill-others \"nodemon app/server\" \"node webpack.server.js\" \"gulp css:watch\"",
    "prod":"npm run build && REDIS_HOST=localhost NODE_ENV=production PORT=3005 NODE_PATH=./app node app/server",
    "webpack": "webpack",
    "bower": "bower install",
    "build": "rimraf ./dist/ && npm run bower && NODE_ENV=production npm run webpack --optimize-minimize -p && gulp prod && node scripts/writeVersion",
    "flow": "yarn run flow",
    "version": "node scripts/writeVersion",
    "install-prod": "rimraf ./node_modules/ && REDIS_HOST=localhost NPM_CONFIG_LOGLEVEL=error NPM_CONFIG_PRODUCTION=true NODE_VERBOSE=false NODE_ENV=production NODE_MODULES_CACHE=true yarn && REDIS_HOST=localhost NODE_ENV=production PORT=3005 NODE_PATH=./app node app/server",
    "kill": "lsof -i tcp:3002 | awk 'NR!=1 {print $2}' | xargs kill -9 | true;"
  },
